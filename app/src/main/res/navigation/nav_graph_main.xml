<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_main"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.dev.clevertonsantos.mybeats.ui.home.HomeFragment"
        android:label=""
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_detailsFragment"
            app:destination="@id/detailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailsFragment"
        android:name="com.dev.clevertonsantos.mybeats.ui.details.DetailsFragment"
        android:label="">

        <argument
            android:name="image"
            app:argType="string"
            app:nullable="false"/>
        <argument
            android:name="name"
            app:argType="string"
            app:nullable="false"/>
        <argument
            android:name="connection"
            app:argType="string"
            app:nullable="false"/>
        <argument
            android:name="compatibility"
            app:argType="string"
            app:nullable="false"/>
        <argument
            android:name="charge"
            app:argType="string"
            app:nullable="false"/>
        <argument
            android:name="autonomy"
            app:argType="string"
            app:nullable="false"/>
        <argument
            android:name="height"
            app:argType="string"
            app:nullable="false"/>
        <argument
            android:name="capture"
            app:argType="string"
            app:nullable="false"/>

    </fragment>

</navigation>